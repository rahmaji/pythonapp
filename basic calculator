num1=int(input("enter the first number"))
num2=int(input("enter the secound number"))
ops=input("enter the operator")
if(ops=="+"):
 print(num1+num2)   
 elif(ops=="-"):
 print(num1-num2)
 elif(ops=="*"):
 print(num1*num2)
 elif(ops=="/"):
  print(num1/num2)
   else:
   print(invalid operator)
                
